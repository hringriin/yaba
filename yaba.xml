<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.blizzard.com/wow/ui/">
    <Script file="yaba.lua"/>
    <!-- Frame to handle all core events -->
    <Frame name="yaba">
        <Scripts>
            <OnLoad>yaba:OnLoad(self)</OnLoad>
            <OnEvent>yaba:eventHandler(self, event, ...)</OnEvent>
        </Scripts>
    </Frame>

    <GameTooltip name="YABA_Tooltip" inherits="GameTooltipTemplate"/>

</Ui>
